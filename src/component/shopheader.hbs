{{#with navigation}}
    <div class='card'>
      <div class='nav'>
        {{#each links}}
          <div class='nav-item'>
            <a href='{{href}}' class='nav-link'>{{text}}</a>
          </div>
        {{/each}}
          <div class='nav-item'>
            <a href="{{href}}" class='nav-link disabled'>Disabled</a>
          </div>
        </div>
      </div>
    </div>
  {{/with}}